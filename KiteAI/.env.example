# Example environment variables for KiteAI
KITEAI_API_PORT=8000
KITEAI_LOG_LEVEL=info
KITEAI_MODEL_PATH=./models/kite-model.pt
DATABASE_URL=sqlite:///./data/kiteai.db
KITEAI_SECRET_KEY=REPLACE_ME_WITH_RANDOM_STRING
DOCKER_REGISTRY=ghcr.io/your-org
DOCKER_IMAGE=kiteai
CI_DOCKER_USERNAME=
CI_DOCKER_PASSWORD=
SENTRY_DSN=
KITEAI_ENABLE_METRICS=false
KITEAI_METRICS_PORT=9090
KITEAI_ENV=development
KITEAI_DEBUG=true
# Fill in real secrets before deploying to production
# Use a secure secret manager for CI/CD; do not commit real credentials